# Bundled Postgres container credentials (installer generates password if blank)
POSTGRES_USER=jarvis
POSTGRES_PASSWORD=
POSTGRES_DB=jarvis

# Agent/database connection string (installer derives from POSTGRES_* if blank)
DATABASE_URL=

# Bundled Redis container credentials (installer generates password if blank)
REDIS_PASSWORD=

# Agent/queue Redis connection string (installer derives from REDIS_PASSWORD if blank)
REDIS_URL=

# Required for dashboard authentication
DASHBOARD_TOKEN=

# Optional fallback. Prefer entering key in dashboard setup wizard.
OPENROUTER_API_KEY=

# Optional dashboard port mapping on host
DASHBOARD_PORT=3001

# Required for GitHub OAuth setup wizard
GITHUB_OAUTH_CLIENT_ID=
GITHUB_OAUTH_CLIENT_SECRET=
# Default Docker callback if dashboard runs on host port 3001:
# http://localhost:3001/setup/github/callback
GITHUB_OAUTH_REDIRECT_URI=

# Optional identity-vault encryption key (recommended)
CREDENTIAL_ENCRYPTION_KEY=

# Optional AI model overrides
# JARVIS_MODEL_STRONG=anthropic/claude-opus-4.6
# JARVIS_MODEL_MID=anthropic/claude-sonnet-4.5
# JARVIS_MODEL_CHEAP=x-ai/grok-4.1-fast

# Optional Discord notifications
# DISCORD_BOT_TOKEN=
# DISCORD_OPERATOR_USER_ID=

# Optional browser automation
# TWO_CAPTCHA_API_KEY=
